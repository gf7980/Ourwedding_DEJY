<script>
		function kCalendar(id, date) {
	var kCalendar = document.getElementById(id);


	if( typeof( date ) !== 'undefined' ) {
		date = date.split('-');
		date[1] = date[1] - 1;
		date = new Date(date[0], date[1], date[2]);
	} else {
		var date = new Date();
	}

	var currentYear = date.getFullYear();
	//ë…„ë„ë¥¼ êµ¬í•¨

	var currentMonth = date.getMonth() + 1;
	//ì—°ì„ êµ¬í•¨. ì›”ì€ 0ë¶€í„° ì‹œì‘í•˜ë¯€ë¡œ +1, 12ì›”ì€ 11ì„ ì¶œë ¥

	var currentDate = date.getDate();
	//ì˜¤ëŠ˜ ì¼ì.

	date.setDate(1);
	var currentDay = date.getDay();
	//ì´ë²ˆë‹¬ 1ì¼ì˜ ìš”ì¼ì€ ì¶œë ¥. 0ì€ ì¼ìš”ì¼ 6ì€ í† ìš”ì¼

	var dateString = new Array('sun', 'mon', 'tue', 'wed', 'thu', 'fri', 'sat');
	var lastDate = new Array(31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31);
	if( (currentYear % 4 === 0 && currentYear % 100 !== 0) || currentYear % 400 === 0 )
		lastDate[1] = 29;
	//ê° ë‹¬ì˜ ë§ˆì§€ë§‰ ì¼ì„ ê³„ì‚°, ìœ¤ë…„ì˜ ê²½ìš° ë…„ë„ê°€ 4ì˜ ë°°ìˆ˜ì´ê³  100ì˜ ë°°ìˆ˜ê°€ ì•„ë‹ ë•Œ í˜¹ì€ 400ì˜ ë°°ìˆ˜ì¼ ë•Œ 2ì›”ë‹¬ì´ 29ì¼ ì„.

	var currentLastDate = lastDate[currentMonth-1];
	var week = Math.ceil( ( currentDay + currentLastDate ) / 7 );
	//ì´ ëª‡ ì£¼ì¸ì§€ êµ¬í•¨.

	if(currentMonth != 1)
		var prevDate = currentYear + '-' + ( currentMonth - 1 ) + '-' + currentDate;
	else
		var prevDate = ( currentYear - 1 ) + '-' + 12 + '-' + currentDate;
	//ë§Œì•½ ì´ë²ˆë‹¬ì´ 1ì›”ì´ë¼ë©´ 1ë…„ ì „ 12ì›”ë¡œ ì¶œë ¥.

	if(currentMonth != 12)
		var nextDate = currentYear + '-' + ( currentMonth + 1 ) + '-' + currentDate;
	else
		var nextDate = ( currentYear + 1 ) + '-' + 1 + '-' + currentDate;
	//ë§Œì•½ ì´ë²ˆë‹¬ì´ 12ì›”ì´ë¼ë©´ 1ë…„ í›„ 1ì›”ë¡œ ì¶œë ¥.


	if( currentMonth < 10 )
		var currentMonth = '0' + currentMonth;
	//10ì›” ì´í•˜ë¼ë©´ ì•ì— 0ì„ ë¶™ì—¬ì¤€ë‹¤.

	var calendar = '';

	calendar += '<div id="header">';
	calendar += '			<span><a href="#" class="button left" onclick="kCalendar(\'' + id + '\', \'' + prevDate + '\')"><</a></span>';
	calendar += '			<span id="date">' + 2022 + 'ë…„ ' + 06 + 'ì›” </span>';
	calendar += '			<span><a href="#" class="button right" onclick="kCalendar(\'' + id + '\', \'' + nextDate + '\')">></a></span>';
	calendar += '		</div>';
	calendar += '		<table border="0" cellspacing="0" cellpadding="0">';
	calendar += '			<caption>' + 2022 + 'ë…„ ' + 06 + 'ì›” ë‹¬ë ¥</caption>';
	calendar += '			<tbody>';

	var dateNum = 1 - currentDay;

	for(var i = 0; i < week; i++) {
		calendar += '			<tr>';
		for(var j = 0; j < 7; j++, dateNum++) {
			if( dateNum < 1 || dateNum > currentLastDate ) {
				calendar += '				<td class="' + dateString[j] + '"> </td>';
				continue;
			}
			if(dateNum == "19"){
			calendar += '				<td class="' + dateString[j] + ' weddingday"><div>' + dateNum + '</div></td>';
			}else{
			calendar += '				<td class="' + dateString[j] + '">' + dateNum + '</td>';
			}

		}
		calendar += '			</tr>';
	}

	calendar += '			</tbody>';
	calendar += '		</table>';

	kCalendar.innerHTML = calendar;
}</script>
	
	
