function eventcnt(num) {

  $.ajax({
    url: '/login/event_proc.php',
    data : 'name='+num,
    cache: false,
    dataType: 'json',
    method: "post",
    timeout: 10000,
    beforeSend : function() {},
    success: function(json)
    {
      var status = json.status;
      var msg = json['msg'];
      if (status == 'success'){
        json_msg(msg);
      }
      return;
    }
  });
}
</script>



